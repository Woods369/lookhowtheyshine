{% style %}
  header {
    box-shadow: 0px 10px 15px -3px rgba(0,0,0,0);
    svg {
      color: white;
      transition: all 300ms ease-in-out;
    }
  }

  header.scrolled {
    background-color: white;
    box-shadow: 0px 10px 15px -3px rgba(0,0,0,0.1);

    svg {
      color: black;
    }
  }

  body:not(.home) > div > header {
    background-color: white;

    svg {
      color: black;
    }
  }

  body.home > div > header.open {
    svg#logo {
      color: white !important;
    }
  }


  header.open {
    svg {
      color: black;
    }

    .menu-screen {
      left: 0;
    }
  
  }

{% endstyle %}

{% javascript %}
  let header = document.querySelector('header');
  let menu = document.querySelector('.menu');
  let menuScreen = document.querySelector('.menu-screen');
  
  // Flags 
  let is_open = false;

  gsap.registerPlugin(MorphSVGPlugin);

    menu.addEventListener('click', (event) => {
      header.classList.toggle('open');

      if (!is_open) {
        gsap.to("#menu-1", { duration: 1, morphSVG: "#menu-close-1" });
        gsap.to('#menu-2', { duration: 1, morphSVG: "#menu-close-2" });

        is_open = true;
      } else {
        gsap.to("#menu-1", { duration: 1, morphSVG: "#menu-1" });
        gsap.to("#menu-2", { duration: 1, morphSVG: "#menu-2" })

        is_open = false;
      }
  })

  window.addEventListener('scroll', (event) => {
    if (window.scrollY > 50) {
      header.classList.add('scrolled');
    } else {
      header.classList.remove('scrolled');
    }
  });

{% endjavascript %}

{% schema %}
  {
    "name": "t:general.header",
    "tag": "header",
    "class": "fixed top-0 z-50 w-full grid items-center transition-all transition-400 py-5",
    "settings": [
      {
        "type": "link_list",
        "id": "menu",
        "label": "t:labels.menu"
      },
      {
        "type": "range",
        "min": 25,
        "default": 50,
        "unit": "px",
        "step": 5,
        "max": 250,
        "id": "logo-size",
        "label" : "t:labels.logo-size"
      }
    ]
  }
{% endschema %}
